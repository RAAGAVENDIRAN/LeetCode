PYTHON:

class Solution:
    def searchInsert(self, nums: List[int], t: int) -> int:
        for i in range(len(nums)):
            if nums[i]>=t:
                return i
        return len(nums)
        
        
C++:

class Solution {
public:
    int searchInsert(vector<int>& nums, int target) {
        for(int i=0;i<nums.size();i++){
            if (nums[i]>=target)
                return i;
        }
        return nums.size();
        
    }
